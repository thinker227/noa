{
    "files.exclude": {
        "src/**/bin/": true,
        "src/**/obj/": true,
        "**/target/": true
    },
    "explorer.fileNesting.enabled": true,
    "explorer.fileNesting.patterns": {
        "package.json": "package-lock.json",
        "Cargo.toml": "Cargo.lock",
        "*.noa": "${capture}.ark"
    },
    "[csharp]": {
        "editor.formatOnType": false
    }
}
